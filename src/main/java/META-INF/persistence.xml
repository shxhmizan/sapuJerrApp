<?xml version="1.0" encoding="UTF-8"?>
<persistence version="3.0" xmlns="https://jakarta.ee/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd">
	<persistence-unit name="SapuJerrApp">
		<provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
		<jta-data-source>java:openejb/Resource/sapujerrDB</jta-data-source>
		<class>model.BookingEntity</class>
		<class>model.CarEntity</class>
		<class>model.DriverEntity</class>
		<class>model.NotificationEntity</class>
		<class>model.SubscriptionPackageEntity</class>
		<class>model.PaymentEntity</class>
		<class>model.PricingRateEntity</class>
		<class>model.ReviewEntity</class>
		<class>model.StudentEntity</class>
		<class>model.SubscriptionEntity</class>
		<class>model.UserEntity</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
	</persistence-unit>
</persistence>
